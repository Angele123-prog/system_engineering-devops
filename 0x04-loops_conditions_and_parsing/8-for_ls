#!/usr/bin/env bash

for file in *; do
    # Skip hidden files (starting with .)
    if [[ "$file" == .* ]]; then
        continue
    fi

    # If filename contains a dash, print the part after the first dash
    if [[ "$file" == *-* ]]; then
        echo "${file#*-}"
    else
        # If no dash, print the full filename
        echo "$file"
    fi
done
